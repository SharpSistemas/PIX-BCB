<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<LangVersion>10.0</LangVersion>
		<Nullable>warnings</Nullable>
		<ImplicitUsings>disable</ImplicitUsings>
		<Version>0.9.1</Version>
		<Authors>RafaelEstevam.Net</Authors>
		<Company>SHARP SISTEMAS LTDA</Company>
		<Product>C# Models for PIX</Product>
		<Description>C# models for PIX</Description>
		<PackageLicenseExpression>MIT</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/SharpSistemas/PIX-BCB</PackageProjectUrl>
		<RepositoryUrl>https://github.com/SharpSistemas/PIX-BCB</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageTags>BCB PIX</PackageTags>
		<PackageReleaseNotes>https://github.com/SharpSistemas/PIX-BCB</PackageReleaseNotes>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<NoWarn>$(NoWarn);1591</NoWarn>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="Newtonsoft.Json" Version="13.0.2" />
	</ItemGroup>

</Project>
